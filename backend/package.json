{
    "name": "ts-express",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "tsx watch ./src/index.ts",
        "build": "tsc",
        "start": "node ./build/index.js",
        "test:unit": "jest --watchAll --verbose --silent=false --testPathPattern=/unit",
        "test:e2e": "jest --watchAll --verbose --silent=false --testPathPattern=/e2e",
        "test:path": "jest --watchAll --verbose ",
        "test": "jest --watchAll --verbose",
        "migrate": "sequelize-cli db:migrate",
        "migrate:undo": "sequelize-cli db:migrate:undo",
        "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
        "migrate:refresh": " npm run migrate:undo:all && npm run migrate",
        "make:model": "sequelize-cli model:generate --models-path \"./src/models\" --name",
        "make:seed": "sequelize-cli seed:generate --seeders-path \"./seeders\" --name",
        "seed": "sequelize-cli db:seed:all",
        "seed:run": "sequelize-cli db:seed --seed",
        "quick:init": "npm run migrate:refresh && npm run seed"
    },
    "author": "GXiang",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.1.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "helmet": "^7.0.0",
        "http": "^0.0.1-security",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "moment-timezone": "^0.5.43",
        "morgan": "^1.10.0",
        "mysql2": "^3.5.2",
        "node-uuid": "^1.4.8",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "sequelize": "^6.32.1",
        "sequelize-cli": "^6.6.1",
        "uuid": "^9.0.0",
        "yup": "^1.2.0"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.22.9",
        "@babel/preset-typescript": "^7.22.5",
        "@types/bcrypt": "^5.0.0",
        "@types/compression": "^1.7.2",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.3",
        "@types/lodash": "^4.14.196",
        "@types/moment": "^2.13.0",
        "@types/moment-timezone": "^0.5.30",
        "@types/morgan": "^1.9.4",
        "@types/node": "^20.4.6",
        "@types/passport": "^1.0.12",
        "@types/passport-jwt": "^3.0.9",
        "@types/passport-local": "^1.0.35",
        "@types/sequelize": "^4.28.15",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "^9.0.2",
        "@types/yup": "^0.32.0",
        "jest": "^29.6.2",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node-dev": "^2.0.0",
        "tsx": "^3.12.7",
        "typescript": "^5.1.6"
    }
